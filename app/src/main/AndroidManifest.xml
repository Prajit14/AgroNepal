<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agronepal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AgroNepal">
        <activity android:name=".blog.Potato"></activity>
        <activity android:name=".blog.Maize" />
        <activity android:name=".blog.Wheat" />
        <activity android:name=".blog.Rice" />
        <activity android:name=".blog.MainBlogList" />
        <activity android:name=".agronews.news" />
        <activity android:name=".News.newspage" />
        <activity android:name=".Dashboard.Dashboard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>